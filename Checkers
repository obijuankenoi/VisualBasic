        ElseIf playerPiece = "W7" And whiteSevenCurrentLocationX = G6X And whiteSevenCurrentLocationY = G6Y Then
            picWhite7.Location = New Point(H5X, H5Y)
            whiteSevenCurrentLocationX = H5X
            whiteSevenCurrentLocationY = H5Y
        End If
    End Sub
    Private Sub btnReset_Click(sender As Object, e As EventArgs) Handles btnReset.Click
        picWhite1.Location = New Point(whiteOneStartingLocationX, whiteOneStartingLocationY)
        whiteOneCurrentLocationX = whiteOneStartingLocationX
        whiteOneCurrentLocationY = whiteOneStartingLocationY

        picWhite2.Location = New Point(whiteTwoStartingLocationX, whiteTwoStartingLocationY)
        whiteTwoCurrentLocationX = whiteTwoStartingLocationX
        whiteTwoCurrentLocationY = whiteTwoStartingLocationY

        picWhite3.Location = New Point(whiteThreeStartingLocationX, whiteThreeStartingLocationY)
        whiteThreeCurrentLocationX = whiteThreeStartingLocationX
        whiteThreeCurrentLocationY = whiteThreeStartingLocationY

        picWhite4.Location = New Point(whiteFourStartingLocationX, whiteFourStartingLocationY)
        whiteFourCurrentLocationX = whiteFourStartingLocationX
        whiteFourCurrentLocationY = whiteFourStartingLocationY

        picWhite5.Location = New Point(whiteFiveStartingLocationX, whiteFiveStartingLocationY)
        whiteFiveCurrentLocationX = whiteFiveStartingLocationX
        whiteFiveCurrentLocationY = whiteFiveStartingLocationY

        picWhite6.Location = New Point(whiteSixStartingLocationX, whiteSixStartingLocationY)
        whiteSixCurrentLocationX = whiteSixStartingLocationX
        whiteSixCurrentLocationY = whiteSixStartingLocationY

        picWhite7.Location = New Point(whiteSevenStartingLocationX, whiteSevenStartingLocationY)
        whiteSevenCurrentLocationX = whiteSevenStartingLocationX
        whiteSevenCurrentLocationY = whiteSevenStartingLocationY
    End Sub
End Class
