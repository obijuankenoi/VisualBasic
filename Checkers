    'Column A
    Private Sub btnA2_Click(sender As Object, e As EventArgs) Handles btnA2.Click
        If isKing = False Then
            If playerPiece = "W1" And whiteOneCurrentLocationX = B3X And whiteOneCurrentLocationY = B3Y Then
                whiteOneMove(A2X, A2Y)
            ElseIf playerPiece = "W2" And whiteTwoCurrentLocationX = B3X And whiteTwoCurrentLocationY = B3Y Then
                whiteTwoMove(A2X, A2Y)
            ElseIf playerPiece = "W3" And whiteThreeCurrentLocationX = B3X And whiteThreeCurrentLocationY = B3Y Then
                whiteThreeMove(A2X, A2Y)
            ElseIf playerPiece = "W5" And whiteFiveCurrentLocationX = B3X And whiteFiveCurrentLocationY = B3Y Then
                whiteFiveMove(A2X, A2Y)
            ElseIf playerPiece = "W6" And whiteSixCurrentLocationX = B3X And whiteSixCurrentLocationY = B3Y Then
                whiteSixMove(A2X, A2Y)
            ElseIf playerPiece = "W7" And whiteSevenCurrentLocationX = B3X And whiteSevenCurrentLocationY = B3Y Then
                whiteSevenMove(A2X, A2Y)
            ElseIf playerPiece = "W9" And whiteNineCurrentLocationX = B3X And whiteNineCurrentLocationY = B3Y Then
                whiteNineMove(A2X, A2Y)
            ElseIf playerPiece = "W10" And whiteTenCurrentLocationX = B3X And whiteTenCurrentLocationY = B3Y Then
                whiteTenMove(A2X, A2Y)
            ElseIf playerPiece = "W11" And whiteElevenCurrentLocationX = B3X And whiteElevenCurrentLocationY = B3Y Then
                whiteElevenMove(A2X, A2Y)
            End If
        End If
    End Sub
